{
    "manifest_version": 3,
    "name": "Enable Password Manager on MyTax Hasil",
    "version": "1.1",
    "description": "Enables Password Manager autofill on mytax.hasil.gov.my",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "permissions": [],
    "content_scripts": [
        {
            "matches": ["https://*.hasil.gov.my/*"],
            "js": ["content.js"],
            "run_at": "document_start"
        }
    ]
}